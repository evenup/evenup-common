{
  "name": "evenup-common",
  "version": "1.1.0",
  "author": "Justin Lambert",
  "summary": "Module to manage packages, services, files, users, and groups not worthy of their own module",
  "license": "Apache-2.0",
  "source": "https://github.com/evenup/evenup-common",
  "project_page": "https://github.com/evenup/evenup-common",
  "issues_url": "https://github.com/evenup/evenup-common/issues",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.6.0 <5.0.0"
    }
  ],
  "operatingsystem_support": [
    { "operatingsystem": "RedHat", "operatingsystemrelease": [ "6", "7" ] },
    { "operatingsystem":"CentOS", "operatingsystemrelease":[ "6", "7" ] }
  ],
  "dependencies": [
    { "name": "evenup/beaver", "version_requirement": ">= 1.3.0 <2.0.0" },
    { "name": "puppetlabs/stdlib", "version_requirement": ">=4.2.0 <5.0.0" },
    { "name": "puppetlabs/firewall", "version_requirement": ">= 1.2.0 <2.0.0" }
  ]
}
